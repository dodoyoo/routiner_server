{
  "name": "routiner_server",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "dev": "env-cmd -f .env.development nodemon --config nodemon.json --exec ts-node server",
    "seed:routines": "env-cmd -f .env.development ts-node ./src/database/seeds/routineSeed.ts",
    "typeorm": "env-cmd -f .env.development ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource ./src/models/dataSource.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "npm run typeorm migration:generate ./src/database/migrations/Migration",
    "migration:run": "npm run typeorm  migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.12.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mysql2": "^3.15.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.27",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.6.1",
    "@types/nodemon": "^3.1.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "env-cmd": "^10.1.0",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2"
  }
}
